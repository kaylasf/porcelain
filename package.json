{
  "name": "porcelain",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'porcelain/*'\" \"npm run porcelain\"",
    "porcelain": "cd porcelain && npm run start",
    "install": "cd porcelain; npm i",
    "build": "cd porcelain && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjanders6/porcelain.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mjanders6/porcelain/issues"
  },
  "homepage": "https://github.com/mjanders6/porcelain#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "mysql2": "^1.6.5",
    "sequelize": "^5.8.6"
  }
}
